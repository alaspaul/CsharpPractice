@using VIewComponentsPractice.Models
@model PersonGridModel

<div class="box">
    <h3>@Model.GridTitle</h3>

    <table class="table w-100">
        <thead>
            <tr>
                <th>SL. no</th>
                <th>Name</th>
            </tr>
        </thead>
        <tbody>
            @foreach(Person person in Model.Persons)
            {

                <tr>
                    <td>@person.Name</td>
                    <td>@person.Age</td>
                </tr>
            }

        </tbody>
    </table>
</div>